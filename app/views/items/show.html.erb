<h1>ジャンル検索</h1><br>
<% @categories.each do |category| %>
    <tr>
      <td><h2><%= link_to category.name, end_users_categories_show_path(category) %></h2></td><br>
    </tr>
<% end %>

<div class = "item-image">
	<%= attachment_image_tag @item, :image, :fill, 200, 200, class: "", fallback: ""%>
</div>

<td><%= @item.name %></td><br>
<td><%= @item.discription %></td><br>
<td>¥</td><td><%= (@price_zeikomi).to_i %></td><td>(税込)</td>
<br>

<%= form_for (@item),url: carts_add_path do |f| %>

<% if end_user_signed_in? %>
<%= f.submit "カートに入れる" %>
<% end %>
<% end %>

