<h1>商品一覧画面</h1>
<thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <br>
  <tbody>
      <% @items.each do |item| %>
    <tr>
      <td><%= item.id %></td>
      <td><%= item.name %></td>
      <td>category表示するところ</td>
      <% if item.status = true %>
      	<td>販売中</td>
      <% else %>
        <td>販売停止中</td>
      <% end %>
    </tr>
  <% end %>
</tbody>