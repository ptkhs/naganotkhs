<div class="container">
<h2>注文一覧画面</h2>
  <table>
  	<thead>
  	<tr>
  		<th>購入日時</th>
  		<th>購入者</th>
  		<th>注文個数</th>
  		<th>注文ステータス</th>
  	</tr>
  	</thead>
  	<tbody>
  		<% @orders.each do |order| %>
    <tr>
  		<td><%= link_to admins_orders_show_path(order) do %>
        <%= order.created_at %></td>
        <% end %>
  		<td><%= order.name %></td>
  		<td><%= order.allquantity %></td>
  		<td><%= order.status %></td>
  		<% end %>
    </tr>
  	</tbody>
  </table>
  <%= paginate @orders %>
</div>

