<h2>注文一覧画面</h2>
  <table>
  	<thead>
  	<tr>
  		<th>購入日時</th>
  		<th>購入者</th>
  		<th>注文個数</th>
  		<th>注文ステータス</th>
  	</tr>
  	</thead>
  	<tbody>
  		<% @orders.each do |order| %>
  		<tr><%= order.created_at %>
  			<%= link_to admins_orders_show_path(@order) %></tr>
  		<tr><%= order.end_user_lastname + order.end_user_firstname %></tr>
  		<tr><%= order.order_detail.item_quantity %></tr>
  		<tr><%= order.status %></tr>
  		<% end %>
  	</tbody>
  </table>
  <%= paginate @orders %>

