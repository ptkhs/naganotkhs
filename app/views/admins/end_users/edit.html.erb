
<div class="container">
    <div class="row">
        <div class="col-xs-8 col-xs-offset-2">
<%= form_for @end_user, url: admins_end_users_show_path do |f| %>
    <h2>会員情報編集画面</h2>
    <Label>氏名</Label>
        <%= f.text_field :lastname, autofocus: true, class:"form-control", id:"inputname", placeholder:"姓"%>
        <%= f.text_field :firstname, autofocus: true, class:"form-control", id:"inputname", placeholder:"名"%>
        <Label>フリガナ</Label>
        <%= f.text_field :lastname_kana, autofocus: true, class:"form-control", id:"inputname", placeholder:"姓(カナ)"%>
        <%= f.text_field :firstname_kana, autofocus: true, class:"form-control", id:"inputname", placeholder:"名(カナ)"%>
        <Label>郵便番号</Label>
    <%= f.number_field :zipcode, autofocus: true, class:"form-control", id:"inputname", placeholder:"郵便番号"%>
    <Label>住所</Label>
    <%= f.text_field :address, autofocus: true, class:"form-control", id:"inputname", placeholder:"住所"%>
    <Label>電話番号</Label>
    <%= f.number_field :phone_number, autofocus: true, class:"form-control", id:"inputname", placeholder:"電話番号"%>
    <Label>メールアドレス</Label>
        <%= f.email_field :email, autofocus: true, class:"form-control", id:"inputEmail", placeholder:"email"%>
  <%= f.radio_button :status, :true %>
  <%= f.label :status, "有効", {value: :true, style: "display: inline-block;"} %>
  <%= f.radio_button :status, :false %>
  <%= f.label :status, "退会済み", {value: :false, style: "display: inline-block;"} %>
    <%= f.submit "編集内容を保存する", class:"btn btn-info btn-block" %>
<% end %>
<div class="col-xs-2">
    </div>
</div>
</div>
</div>