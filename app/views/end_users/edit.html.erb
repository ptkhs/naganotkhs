
<div class="container">
    <div class="row">
        <div class="col-xs-7">
            <td><%= link_to "退会する", end_users_bye_path , class: "btn btn-danger" %></td>
<%= form_for @end_user,url: end_users_edit_path do |f| %>
    <h2>会員編集画面</h2>
    <table class ="table">
        <tr>
    <td><Label>姓</Label></td>
        <td><%= f.text_field :lastname, autofocus: true, class:"form-control", id:"inputname", placeholder:"姓"%></td>
    </tr>
    <tr>
        <td>
        <Label>名</Label></td>
        <td><%= f.text_field :firstname, autofocus: true, class:"form-control", id:"inputname", placeholder:"名"%></td>
    </tr>
    <tr>
        <td><Label>姓(カナ)</Label></td>
        <td><%= f.text_field :lastname_kana, autofocus: true, class:"form-control", id:"inputname", placeholder:"姓(カナ)"%></td>
    </tr>
    <tr>    
        <td><Label>名(カナ)</Label></td>
        <td><%= f.text_field :firstname_kana, autofocus: true, class:"form-control", id:"inputname", placeholder:"名(カナ)"%></td>
    </tr>
    <tr>
        <td><Label>郵便番号</Label></td>
    <td><%= f.number_field :zipcode, autofocus: true, class:"form-control", id:"inputname", placeholder:"郵便番号"%></td>
</tr>
    <tr>
    <td><Label>住所</Label></td>
    <td><%= f.text_field :address, autofocus: true, class:"form-control", id:"inputname", placeholder:"住所"%></td>
</tr>
<tr>
    <td><Label>電話番号</Label></td>
    <td><%= f.number_field :phone_number, autofocus: true, class:"form-control", id:"inputname", placeholder:"電話番号"%></td>
</tr>
<tr>
    <td><Label>メールアドレス</Label></td>
        <td><%= f.email_field :email, autofocus: true, class:"form-control", id:"inputEmail", placeholder:"email"%></td>
</tr>
<tr><td>
    <%= f.submit "編集内容を保存する", class:"btn btn-primary" %>
</td></tr>
<% end %>
</div>
</div>
</div>